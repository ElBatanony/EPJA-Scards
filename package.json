{
  "name": "uds-scards",
  "version": "0.5.0",
  "description": "Scards project for EPJA",
  "main": "./src/main.tsx",
  "scripts": {
    "start": "cross-env VERSION=master ijl-cli --server --port=8099 --with-open-browser",
    "start:nobrowserstart:not:open:browser": "ijl-cli --server --port=8099",
    "start:nobrowser": "npm run start:nobrowserstart:not:open:browser",
    "build": "npm run clean && ijl-cli --build --dev",
    "build:prod": "npm run clean && ijl-cli --build",
    "clean": "rimraf dist && rimraf coverage",
    "test": "jest",
    "test:ts": "tsc --noEmit",
    "cy:test": "cypress run",
    "lint:ts": "tslint --project .",
    "ijl:check:update": "npx ncu '/^@ijl/.*$/'",
    "ijl:update": "npx ncu '/^@ijl/.*$/' -u && npm i -d",
    "cy:open": "cypress open"
  },
  "license": "ISC",
  "dependencies": {
    "@fluentui/react": "^7.135.0",
    "@types/react": "^16.9.49",
    "@types/react-dom": "^16.9.8",
    "@uifabric/react-cards": "^0.112.5",
    "axios": "^0.20.0",
    "cross-env": "^7.0.2",
    "office-ui-fabric-react": "^7.135.0",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "react-router-dom": "^5.2.0"
  },
  "devDependencies": {
    "@babel/plugin-proposal-class-properties": "^7.8.3",
    "@babel/plugin-transform-runtime": "^7.9.0",
    "@babel/preset-typescript": "^7.9.0",
    "@ijl/cli": "^1.8.2",
    "@types/jest": "^25.2.1",
    "@types/node": "13.7.0",
    "express": "4.17.1",
    "jest": "^25.2.7",
    "rimraf": "^3.0.2",
    "tslint": "^6.1.3",
    "typescript": "^4.0.2"
  }
}
